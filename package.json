{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run tailwind-prod && npm run build:remix",
    "build:remix": "remix build",
    "dev": "concurrently \"remix dev\" \"npm run dev:css\"",
    "dev:remix": "remix dev",
    "dev:css": "npm run tailwind-dev",
    "postinstall": "remix setup node",
    "start": "npm run tailwind-prod && npm run start:remix",
    "start:remix": "remix-serve build",
    "tailwind-dev": "tailwindcss --output ./app/styles/tailwind.css --config ./config/tailwind.js --watch",
    "tailwind-prod": "tailwindcss --output ./app/styles/tailwind.css --config ./config/tailwind.js --minify",
    "prisma": "npx prisma generate",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.3",
    "@prisma/client": "^3.6.0",
    "@remix-run/react": "^1.1.0",
    "@remix-run/serve": "^1.1.0",
    "@tailwindcss/forms": "^0.4.0",
    "chart.js": "^3.7.0",
    "dotenv": "^10.0.0",
    "isomorphic-dompurify": "^0.17.0",
    "luxon": "^2.3.0",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.0.0",
    "react-charts": "^3.0.0-beta.34",
    "react-dom": "^17.0.2",
    "remix": "^1.1.0",
    "remix-auth": "^2.5.1",
    "tailwindcss": "^3.0.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@remix-run/dev": "^1.1.0",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/luxon": "^2.0.9",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "babel-jest": "^27.4.6",
    "concurrently": "^6.5.0",
    "jest": "^27.4.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
